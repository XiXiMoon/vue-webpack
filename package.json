    "delopToPre": "cross-env NODE_ENV=pre webpack --config build/webpack.build.conf.js",
    "delopToProd": "cross-env NODE_ENV=prod webpack --config build/webpack.build.conf.js",
    "mock": "node mock/index.js",
    "mock-dev": "node mock/index.js | webpack-dev-server --inline --progress --config ./build/webpack.dev.conf.js"
  },
